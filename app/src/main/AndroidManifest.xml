        <activity
            android:name=".activitys.DailyCardActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing|stateAlwaysHidden"/>
        <activity
            android:name=".activitys.CardDetailActivity"
            android:screenOrientation="portrait"
